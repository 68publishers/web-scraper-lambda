{
    "name": "@68publishers/web-scraper-client",
    "author": "68publishers",
    "description": "Client for 68publishers/web-scraper-lambda.",
    "version": "1.0.1",
    "private": false,
    "main": "./src/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/68publishers/web-scraper-lambda"
    },
    "keywords": [
        "68publishers",
        "web-scraper",
        "scraper",
        "lambda"
    ],
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.20.2",
        "babel-loader": "^9.1.2",
        "eslint": "^8.34.0",
        "eslint-plugin-jest": "^27.2.1",
        "jest": "^29.4.2",
        "terser-webpack-plugin": "^5.3.6",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    },
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "eslint:fix": "eslint ./src ./__tests__ --ext .js,.mjs --fix",
        "eslint": "eslint ./src ./__tests__ --ext .js,.mjs",
        "build": "webpack --config webpack.config.prod.js"
    },
    "jest": {
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/__tests__/**/*.mjs?(x)",
            "**/?(*.)+(spec|test).mjs?(x)"
        ],
        "moduleFileExtensions": [
            "mjs",
            "js"
        ]
    },
    "files": [
        "src",
        "dist/web-scraper-client.min.js"
    ]
}
