{
    "name": "web-scraper-lambda",
    "author": "68publishers",
    "description": "AWS lambda for web content scraping.",
    "version": "1.2.0",
    "private": true,
    "engines": {
        "node": ">=18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/68publishers/web-scraper-lambda"
    },
    "dependencies": {
        "linkedom": "^0.14.22",
        "open-graph-scraper": "^5.1.0"
    },
    "devDependencies": {
        "eslint": "^8.34.0",
        "eslint-plugin-jest": "^27.2.1",
        "jest": "^29.4.2"
    },
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "eslint:fix": "eslint ./src ./__tests__ --ext .js,.mjs --fix",
        "eslint": "eslint ./src ./__tests__ --ext .js,.mjs"
    },
    "jest": {
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/__tests__/**/*.mjs?(x)",
            "**/?(*.)+(spec|test).mjs?(x)"
        ],
        "moduleFileExtensions": [
            "mjs",
            "js"
        ]
    }
}
